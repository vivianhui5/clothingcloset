{
    "manifest_version": 3,
    "name": "Clothing Closet",
    "version": "1.0",
    "description": "Add clothes to your closet!",
    "permissions": [
      "activeTab",
      "scripting",
      "downloads"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "/images/icon.png"
    },
    "icons": {
      "16":  "/images/icon.png",
      "48":  "/images/icon.png",
      "128":  "/images/icon.png"
    }
  }
  